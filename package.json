{
  "name": "se-csproj-extensions",
  "displayName": "csproj Extensions",
  "description": "Intellisense for your csproj files. It will detect your props file and will give you completion for it.",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.46.0"
  },
  "publisher": "selcukermaya",
  "repository": {
    "type": "git",
    "url": "https://github.com/se/vscode-csproj-extensions"
  },
  "bugs": {
    "url": "https://github.com/se/vscode-csproj-extensions/issues"
  },
  "icon": "images/logo.png",
  "author": {
    "name": "Sel√ßuk Ermaya",
    "email": "selcukermaya@gmail.com",
    "url": "https://selcukermaya.com"
  },
  "keywords": [
    "csproj",
    "csharp",
    "extensions",
    "project",
    "csharpproject",
    "visualstudio"
  ],
  "galleryBanner": {
    "color": "#333333",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "pkg": "mkdir -p publish && vsce package --out ./publish/",
    "clean": "git clean -Xdf -e !node_modules -e !node_modules/**/*",
    "pub": "vsce publish",
    "reset": "yarn run clean && yarn --frozen-lockfile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/uuid": "^8.0.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "fast-xml-parser": "^3.17.4",
    "slugify": "^1.4.2",
    "uuid": "^8.2.0"
  }
}
