{
  "name": "se-csproj-extensions",
  "displayName": "csproj Extensions",
  "description": "Intellisense for your csproj files.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.46.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/se/vscode-csproj-extensions"
  },
  "author": {
    "name": "Sel√ßuk Ermaya",
    "email": "selcukermaya@gmail.com",
    "url": "https://selcukermaya.com"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "activationEvents": [
    "onCommand:csproj-extensions.toggleExtensionState"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "csproj-extensions.toggleExtensionState",
        "title": "Enable / Disable csproj Extension"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
